{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.2",
        "barryvdh/laravel-snappy": "^0.4.7",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "freelancehunt/php-credit-card-validator": "^3.2",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^10.0",
        "laravel/scout": "^9.8",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravel/vapor-cli": "^1.24",
        "laravel/vapor-core": "^2.3",
        "laravel/vapor-ui": "^1.3",
        "maatwebsite/excel": "^3.1",
        "pragmarx/google2fa-laravel": "^2.2",
        "spatie/eloquent-sortable": "^3.8",
        "spatie/laravel-permission": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.3.6",
        "h4cc/wkhtmltopdf-amd64": "0.12.4",
        "laravel/homestead": "^12.5",
        "laravel/sail": "^1.11",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": ["barryvdh/laravel-snappy"]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
